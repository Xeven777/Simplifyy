{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/api/url/shorten",
      "methods": ["POST"],
      "headers": ["Origin", "Content-Type", "Accept"],
      "allowCredentials": true,
      "maxAge": 3600
    }
  ]
}
